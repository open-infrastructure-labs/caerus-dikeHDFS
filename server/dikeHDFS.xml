<configuration>
    <dike>
    <property>
        <name>dfs.namenode.http-address</name>
        <value>dikehdfs:9870</value>
    </property>

    <property>
        <name>dfs.datanode.http-port</name>
        <value>9864</value>
    </property>

    <property>
        <name>dike.dfs.ndp.http-port</name>
        <value>9859</value>
    </property>

    <property>
        <name>dike.dfs.gateway.http-port</name>
        <value>9860</value>
    </property> 

    <property>
        <name>dike.S3.endpoint.http-address</name>
        <value>minio:9000</value>
    </property> 

    <property>
        <name>dike.S3.gateway.http-port</name>
        <value>9858</value>
    </property> 

    <property>
        <!-- 0 - Compute Node (default), 1 Storage Node -->
        <name>dike.node.type</name>
        <value>1</value>
    </property> 

    <property>
        <!-- Max concurent requests -->
        <name>dike.storage.max.requests</name>
        <value>64</value>
    </property> 

    <property>
        <!-- Concurent ReadAhead workers per processor -->
        <name>dike.storage.processor.workers</name>
        <value>2</value>
    </property> 

    <property>
        <!-- Concurent ReadAhead loopback will simply report results of first partition -->
        <name>dike.storage.processor.loopback</name>
        <value>0</value>
    </property> 

    </dike>
</configuration>